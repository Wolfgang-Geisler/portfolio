{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.formatted-text",
        "content.projects-list"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "seo.seo"
    },
    "isMainPage": {
      "type": "boolean",
      "default": false,
      "unique": false
    }
  }
}
